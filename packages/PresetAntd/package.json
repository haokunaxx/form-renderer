{
  "name": "@form-renderer/preset-antd",
  "version": "1.0.0-alpha.1",
  "description": "Ant Design preset for form-renderer (React)",
  "type": "module",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "dist/index.d.ts",
      "import": "dist/index.js",
      "default": "dist/index.js"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc --noEmit && vite build",
    "build:lib": "vite build",
    "build:skip-check": "vite build",
    "test": "vitest",
    "test:coverage": "vitest --coverage"
  },
  "dependencies": {
    "@ant-design/icons": "^6.1.0",
    "@form-renderer/adapter-react": "workspace:*",
    "@form-renderer/engine": "workspace:*"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.0",
    "antd": "^5.11.0",
    "dayjs": "^1.11.10",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.0.0"
  },
  "peerDependencies": {
    "@form-renderer/adapter-react": "workspace:*",
    "antd": "^5.0.0",
    "dayjs": "^1.11.0",
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "keywords": [
    "form",
    "form-renderer",
    "react",
    "antd",
    "ant-design",
    "preset"
  ],
  "author": "",
  "license": "MIT"
}
