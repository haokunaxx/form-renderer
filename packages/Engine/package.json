{
  "name": "@form-renderer/engine",
  "version": "1.0.0-alpha.0",
  "description": "一个基于 JSON Schema 的表单引擎",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vitest",
    "test": "vitest run",
    "test:coverage": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "debug": "npx tsx",
    "build": "tsc",
    "build:lib": "tsc --project tsconfig.prod.json",
    "type-check": "tsc --noEmit"
  },
  "keywords": [
    "form",
    "schema",
    "engine",
    "typescript"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "@vitest/ui": "^1.0.0",
    "tsx": "^4.7.0",
    "typescript": "^5.3.0",
    "vitest": "^1.0.0"
  }
}
