{
  "name": "@form-renderer/adapter-vue3",
  "version": "1.0.0-alpha.5",
  "description": "Vue3 adapter for FormEngine",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./src/index.ts",
      "types": "./src/types.ts"
    },
    "./style.css": "./dist/style.css"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build",
    "build:lib": "vite build",
    "build:skip-check": "vite build",
    "test": "vitest",
    "test:coverage": "vitest --coverage",
    "lint": "eslint src --ext .ts,.vue --fix",
    "preview": "vite preview"
  },
  "dependencies": {
    "@form-renderer/engine": "workspace:*",
    "vue": "^3.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@vitejs/plugin-vue": "^5.0.0",
    "@vue/test-utils": "^2.4.0",
    "typescript": "^5.3.0",
    "vite": "^5.0.0",
    "vite-plugin-dts": "^4.5.4",
    "vitest": "^1.0.0",
    "vue-tsc": "^2.0.0"
  },
  "peerDependencies": {
    "vue": "^3.3.0"
  },
  "keywords": [
    "form",
    "form-engine",
    "vue3",
    "adapter",
    "form-renderer"
  ],
  "author": "",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/your-org/form-renderer"
  }
}
